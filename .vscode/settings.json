{
  "oxlint.enable": true,
  "oxc.trace.server": "verbose",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "oxc.oxc-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.oxlint": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "oxc.configPath": "./frontend/.oxlintrc.json",
  "oxc.requireConfig": true,
  "oxc.tsConfigPath": "./frontend/tsconfig.json",
  "oxc.typeAware": true,
  "oxc.path.oxfmt": "./frontend/node_modules/.bin/oxfmt.exe",
  "oxc.path.oxlint": "./frontend/node_modules/.bin/oxlint.exe",
  "oxc.path.tsgolint": "./frontend/node_modules/.bin/tsgolint.exe",
  "vitest.commandLine": "bunx vitest",
  "vitest.enable": true,
  "vitest.nodeExecutable": "C:\\Users\\gaelw\\.bun\\bin\\bun.exe",
  "npm.packageManager": "bun",
  "npm.scriptRunner": "bun"
}
